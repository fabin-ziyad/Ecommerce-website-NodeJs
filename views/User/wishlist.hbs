    <div class="page-wrapper">
        <main class="main">
        	<div class="page-header text-center" style="background-image: url('assets/images/page-header-bg.jpg')">
        		<div class="container">
        			<h1 class="page-title">Wishlist<span>Shop</span></h1>
        		</div><!-- End .container -->
        	</div><!-- End .page-header -->
            <nav aria-label="breadcrumb" class="breadcrumb-nav">
                <div class="container">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                        <li class="breadcrumb-item"><a href="#">Shop</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Wishlist</li>
                    </ol>
                </div><!-- End .container -->
            </nav><!-- End .breadcrumb-nav -->

            <div class="page-content">
            	<div class="container">
					<table class="table table-wishlist table-mobile" id="WishlistTable">
						<thead>
							<tr>
								<th>Product</th>
                                <th>Name</th>
								<th>Price</th>
								<th>Stock Status</th>
								<th class="text-center">View</th>
								<th>Remove</th>
							</tr>
						</thead>

						<tbody>
                            {{#each DataObj.Wishlists_Products}}
							<tr>
								<td class="product-col">
									<div class="product">
										<figure class="product-media">
											<a href="#">
												<img src="assets/img/products/{{this.Product_Id}}/{{this.Product_Id}}1.jpg" alt="Product image" class="img-fluid">
											</a>
										</figure>
                                </td>
                                <td>
										<h3 class="product-title">
											<a href="#">{{this.ProductName}}</a>
										</h3><!-- End .product-title -->
									</div><!-- End .product -->
								</td>
                                
								<td class="price-col">&#8377 {{this.ProductPrice}}</td>
                                {{#if this.InStock}}
								<td class="stock-col"><span class="in-stock">In stock</span></td>
								<td class="action-col">
                                    <a href ="/item?id={{this.Product_Id}}" class="btn btn-block btn-outline-primary-2 ">View Product</a>
								</td>
                                {{else}}
                                <td class="stock-col"><span class="in-stock text-danger">Stock out</span></td>
								<td class="action-col">
                                    <a href ="/item?id={{this.Product_Id}}" class="btn btn-block btn-outline-primary-2 disabled">View Product</a>
								</td>
                                {{/if}}
								<td class="remove-col"><button class="btn-remove" onclick="RemoveFromWishlist('{{../DataObj.User._id}}','{{this.Product_Id}}')"><i class="icon-close"></i></button></td>
							</tr>
                            {{/each}}
						</tbody>
					</table><!-- End .table table-wishlist -->
	            	<div class="wishlist-share">
	            		<div class="social-icons social-icons-sm mb-2">
	            			<label class="social-label">Share on:</label>
	    					<a href="#" class="social-icon" title="Facebook" target="_blank"><i class="icon-facebook-f"></i></a>
	    					<a href="#" class="social-icon" title="Twitter" target="_blank"><i class="icon-twitter"></i></a>
	    					<a href="#" class="social-icon" title="Instagram" target="_blank"><i class="icon-instagram"></i></a>
	    					<a href="#" class="social-icon" title="Youtube" target="_blank"><i class="icon-youtube"></i></a>
	    					<a href="#" class="social-icon" title="Pinterest" target="_blank"><i class="icon-pinterest"></i></a>
	    				</div><!-- End .soial-icons -->
	            	</div><!-- End .wishlist-share -->
            	</div><!-- End .container -->
            </div><!-- End .page-content -->
        </main><!-- End .main -->
    </div><!-- End .page-wrapper -->
    <button id="scroll-top" title="Back to Top"><i class="icon-arrow-up"></i></button>

    <!-- Mobile Menu -->
    <div class="mobile-menu-overlay"></div><!-- End .mobil-menu-overlay -->

    {{>mobile-menu}}

    

